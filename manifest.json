{
    "manifest_version": 3,
    "name": "Stock Scraper",
    "version": "1.0",
    "description": "Scrapes stock data from Investing.com after page load",
    "permissions": ["activeTab", "downloads"],
    "content_scripts": [
    {
        "matches": ["https://*.investing.com/equities/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
    }
    ],
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "48": "icon48.png"
    }
    }
  }